{{ template "page" . }}

{{ define "main" }}
<div class="main">
  <header>
    <h1 class="govuk-heading-l govuk-!-margin-top-0 govuk-!-margin-bottom-0">Timeline</h1>
  </header>
  <div class="govuk-grid-row">
    <div class="govuk-grid-column-two-thirds">
        <div class="moj-timeline">
        {{ range .DeputyEvents }}
           {{ template "timeline-template-renderer" . }}
         {{ end }}
        </div>
    </div>
  </div>
  </div>

{{ end }}